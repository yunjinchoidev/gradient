<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="project5.project.ProjectDao">
	<select id="list" resultType="project5.project.ProjectVO">
		select * from project
	</select>

	<select id="get" resultType="project5.project.ProjectVO"
		parameterType="int">
		select * from project where projectkey = #{projectkey}
	</select>

	<select id="get2" resultType="project5.project.ProjectVO" parameterType="java.util.List">
		select * from project where projectkey
		in
		<foreach collection="list" item="projectkey" open="(" close=")"
			separator=",">
			#{projectkey}
		</foreach>

	</select>


</mapper>
