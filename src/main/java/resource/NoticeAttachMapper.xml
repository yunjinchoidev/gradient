<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
	
<mapper namespace="project5.notice.NoticeDao">
	<select id="getList" resultType="project5.notice.NoticeVO">
		select * from notice
	</select>

	<select id="get" resultType="project5.notice.NoticeVO" parameterType="int">
		select * from notice where noticekey=#{noticekey}
	</select>
	
	<insert id="insert">
		insert into notice values
		(seq_notice.nextval, #{title}, #{content}, 0, sysdate, 1)
	</insert>
	
	
</mapper>
