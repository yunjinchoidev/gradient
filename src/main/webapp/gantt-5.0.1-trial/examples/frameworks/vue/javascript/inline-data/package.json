{
  "name": "inline-data",
  "description": "Inline data in Vue Gantt",
  "version": "5.0.1",
  "author": {
    "name": "Bryntum",
    "url": "https://bryntum.com",
    "email": "info@bryntum.com"
  },
  "license": "commercial",
  "homepage": "https://bryntum.com",
  "scripts": {
    "start": "npm run serve",
    "serve": "cross-env NODE_OPTIONS='--max-old-space-size=8192' NODE_ENV=development vue-cli-service serve",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' NODE_ENV=production vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "postinstall"
  },
  "postinstall": {
    "node_modules/@bryntum/demo-resources/images/favicon.png": "copy public/favicon.png",
    "node_modules/@bryntum/demo-resources/images/users/*": "copy public/users/"
  },
  "dependencies": {
    "@bryntum/demo-resources": "1.1.0",
    "@bryntum/gantt": "npm:@bryntum/gantt-trial@5.0.1",
    "@bryntum/gantt-vue": "5.0.1",
    "vue": "~2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.1.2",
    "@vue/cli-plugin-eslint": "~4.1.2",
    "@vue/cli-service": "~4.1.2",
    "@vue/eslint-config-standard": "~5.0.1",
    "babel-eslint": "~10.0.3",
    "cross-env": "~6.0.3",
    "eslint": "~5.16.0",
    "eslint-plugin-vue": "~5.0.0",
    "postinstall": "~0.7.0",
    "sass": "~1.42.1",
    "sass-loader": "~8.0.2",
    "vue-template-compiler": "~2.6.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {
      "vue/no-parsing-error": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
