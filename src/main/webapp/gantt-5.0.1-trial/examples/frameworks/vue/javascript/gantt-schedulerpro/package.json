    "vue": "~2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "~10.1.0",
    "cross-env": "~7.0.3",
    "eslint": "~6.7.2",
    "eslint-plugin-vue": "~6.2.2",
    "postinstall": "~0.7.0",
    "sass": "~1.26.5",
    "sass-loader": "~8.0.2",
    "vue-template-compiler": "~2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "eslintConfig": {
