{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "bryntum-gantt-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/bryntumGantt/BryntumGanttWebPart.js",
          "manifest": "./src/webparts/bryntumGantt/BryntumGanttWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "@bryntum/gantt": "node_modules/@bryntum/gantt"
  },
  "localizedResources": {
    "BryntumGanttWebPartStrings": "lib/webparts/bryntumGantt/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}
